<view><block wx:if="{{videoUri}}"><video class="video" id="myVideo" src="{{videoUri}}" controls="{{true}}"><cover-view class="video-control"><cover-view data-event-opts="{{[['tap',[['showSwitchRate',['$event']]]]]}}" class="multi rate" bindtap="__e">{{"x "+currentRate}}</cover-view></cover-view><cover-view class="{{['multi-list rate',(rateShow)?'active':'']}}"><block wx:for="{{['0.5','0.8','1.0','1.25','1.5']}}" wx:for-item="item" wx:for-index="index" wx:key="index"><cover-view class="{{['multi-item rate',(item==currentRate)?'active':'']}}" data-rate="{{item}}" data-event-opts="{{[['tap',[['switchRate',['$event']]]]]}}" bindtap="__e">{{''+item+''}}</cover-view></block></cover-view></video></block></view>